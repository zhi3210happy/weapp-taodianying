/**app.wxss**/

@import 'weui.wxss';

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

page{
	background-color: #f4f4f4;
}

.pull-right{
	float:right;
}
.pull-left{
	float:left;
}
.clearfix:after{
	content: '';
	display:block;
	height:0;
	visibility: hidden;
	clear:both;
}